print("Введите 6-значное число")
a = int(input())
b = 2
if (99999<a) and (a<1000000):
    a1 = a//100000
    a2 = a//10000%10
    a3 = a//1000%10%10
    a4 = a%1000//100
    a5 = a%100//10
    a6 = a%10
    #print(a1,a2,a3,a4,a5,a6)
    if (b == a1) or (b == a2) or (b == a3) or (b == a4) or (b == a5) or (b == a6):
        print("Да оно есть")
    else:
        print("Его нет")
else:
    print("6-значное!")